@model IEnumerable<MovieStore.Core.Entities.Movie>
@{
    ViewData["Title"] = "FavoriteMovies";
}

<h1>My Favorite Movies</h1>

<div>
    <div class="container-fluid">
        <div class="row">
            @foreach (var movie in Model)
            {

                @await Html.PartialAsync("_MovieCard", movie);
            }

        </div>

    </div>


</div>
